{
  "name": "automation_checkone-",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
"scripts": {
  "cypress:open": "cypress open",

  "test:smoke": "cypress run --spec \"cypress/e2e/smoke/**/*.cy.js\" --browser chrome --headless --record --parallel --group smoke-chrome --ci-build-id $GITHUB_RUN_ID",
  "test:smoke:firefox": "cypress run --spec \"cypress/e2e/smoke/**/*.cy.js\" --browser firefox --headless --record --parallel --group smoke-firefox --ci-build-id $GITHUB_RUN_ID",

  "test:regression:chunk1": "cypress run --spec \"cypress/e2e/regression/chunk-1/**/*.cy.js\" --browser chrome --headless --record --parallel --group regression-chunk1-chrome --ci-build-id $GITHUB_RUN_ID",
  "test:regression:chunk2": "cypress run --spec \"cypress/e2e/regression/chunk-2/**/*.cy.js\" --browser chrome --headless --record --parallel --group regression-chunk2-chrome --ci-build-id $GITHUB_RUN_ID",
  "test:regression:chunk3": "cypress run --spec \"cypress/e2e/regression/chunk-3/**/*.cy.js\" --browser chrome --headless --record --parallel --group regression-chunk3-chrome --ci-build-id $GITHUB_RUN_ID",

  "test:regression:chunk1:firefox": "cypress run --spec \"cypress/e2e/regression/chunk-1/**/*.cy.js\" --browser firefox --headless --record --parallel --group regression-chunk1-firefox --ci-build-id $GITHUB_RUN_ID",
  "test:regression:chunk2:firefox": "cypress run --spec \"cypress/e2e/regression/chunk-2/**/*.cy.js\" --browser firefox --headless --record --parallel --group regression-chunk2-firefox --ci-build-id $GITHUB_RUN_ID",
  "test:regression:chunk3:firefox": "cypress run --spec \"cypress/e2e/regression/chunk-3/**/*.cy.js\" --browser firefox --headless --record --parallel --group regression-chunk3-firefox --ci-build-id $GITHUB_RUN_ID",

  "test:retest": "cypress run --spec \"cypress/e2e/retest/**/*.cy.js\" --browser chrome --headless --record --parallel --group retest-chrome --ci-build-id $GITHUB_RUN_ID",
  "test:retest:firefox": "cypress run --spec \"cypress/e2e/retest/**/*.cy.js\" --browser firefox --headless --record --parallel --group retest-firefox --ci-build-id $GITHUB_RUN_ID"
},

  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^15.10.0"
  }
}
