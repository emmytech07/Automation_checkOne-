{
  "name": "automation_checkone-",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress:open": "npx cypress open",
    "test:smoke": "npx cypress run --env grepTags=smoke --browser chrome --headless --record --parallel --group smoke-chrome --ci-build-id $GITHUB_RUN_ID",
    "test:smoke:firefox": "npx cypress run --env grepTags=smoke --browser firefox --headless --record --parallel --group smoke-firefox --ci-build-id $GITHUB_RUN_ID",
    "test:regression:chunk1": "npx cypress run --spec \"cypress/e2e/regression/chunk-1/*.cy.js\" --browser chrome --headless --record --parallel --group regression-chunk1-chrome --ci-build-id $GITHUB_RUN_ID",
    "test:regression:chunk2": "npx cypress run --spec \"cypress/e2e/regression/chunk-2/*.cy.js\" --browser chrome --headless --record --parallel --group regression-chunk2-chrome --ci-build-id $GITHUB_RUN_ID",
    "test:regression:chunk3": "npx cypress run --spec \"cypress/e2e/regression/chunk-3/*.cy.js\" --browser chrome --headless --record --parallel --group regression-chunk3-chrome --ci-build-id $GITHUB_RUN_ID",
    "test:regression:chunk1:firefox": "npx cypress run --spec \"cypress/e2e/regression/chunk-1/*.cy.js\" --browser firefox --headless --record --parallel --group regression-chunk1-firefox --ci-build-id $GITHUB_RUN_ID",
    "test:regression:chunk2:firefox": "npx cypress run --spec \"cypress/e2e/regression/chunk-2/*.cy.js\" --browser firefox --headless --record --parallel --group regression-chunk2-firefox --ci-build-id $GITHUB_RUN_ID",
    "test:regression:chunk3:firefox": "npx cypress run --spec \"cypress/e2e/regression/chunk-3/*.cy.js\" --browser firefox --headless --record --parallel --group regression-chunk3-firefox --ci-build-id $GITHUB_RUN_ID",
    "test:retest": "npx cypress run --spec \"cypress/e2e/retest/*.cy.js\" --browser chrome --headless --record --parallel --group retest-chrome --ci-build-id $GITHUB_RUN_ID",
    "test:retest:firefox": "npx cypress run --spec \"cypress/e2e/retest/*.cy.js\" --browser firefox --headless --record --parallel --group retest-firefox --ci-build-id $GITHUB_RUN_ID",
    "simple-run": "npx cypress run --spec cypress/e2e/regression/chunk-1",
    "junit-merge": "npx junit-merge -d cypress/results/junit/results -o cypress/results/junit/results.xml",
    "delete-junit-report": "rm -rf cypress/results/junit/results.xml",
    "delete-results": "rm -rf cypress/results/* || true",
    "mochaweome-merge": "npx mochawesome-merge cypress/results/mochawesome/*.json > mochawesome.json && npx marge mochawesome.json",
    "delete-mocha-report": "rm -rf mochawesome-report/* || true",
    "cypress-regression-pack": " npm run delete-results  && npm run delete-mocha-report && npm run simple-run && npm run mochaweome-merge"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/grep": "^6.0.0",
    "cypress": "^15.10.0",
    "cypress-multi-reporters": "^2.0.5",
    "mocha-junit-reporter": "^2.2.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.1",
    "mochawesome-report-generator": "^6.3.2"
  }
}
